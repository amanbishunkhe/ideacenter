    -o-transform: skewX(-30deg) translateZ(0);
    animation: 0.5s ease-out 0s 1 slideInHeader;
}
.speaker h2.entry-title::after {
    padding: 12px;
    content: '';
    display: block;
    position: absolute;
    -webkit-transform: skewX(-30deg) translateZ(0);
    -moz-transform: skewX(-30deg) translateZ(0);
    -o-transform: skewX(-30deg) translateZ(0);
    transform: skewX(-30deg) translateZ(0);
    -webkit-animation: 0.5s ease-out 0s 1 slideInHeader;
    animation: 0.5s ease-out 0s 1 slideInHeader;
    bottom: -39px;
    height: 0.5em;
    right: 71%;
    left: -100vw;
    background: #b3b4b8;
}
.testimonial-section .entry-title {
