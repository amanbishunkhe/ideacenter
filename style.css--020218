/*    testimonial section styling from here   */
.testimonial-section {
    background: #EBEBEB;
    padding: 95px 0 30px;
}
.testimonial-section,
.speaker,
.speaker h2.entry-title{
    position: relative;
}
.speaker {
    float: left;
