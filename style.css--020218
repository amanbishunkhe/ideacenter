.menu-social-link-container {
    text-align: center;
    margin: 10px 0;
}
li{
    list-style: none;
}
.menu-social-link-container ul{
    padding: 0;
}
.menu-social-link-container li {
    display: inline-block;
    margin: 0 2px 5px;
}
.menu-social-link-container li a {
    background: #dbdbdb none repeat scroll 0 0;
    border-radius: 33px;
    color: #fff;
    display: block;
    font-size: 0;
    height: 45px;
    padding: 1px 0 0;
    text-align: center;
    width: 45px;
}
.menu-social-link-container li a[href*="facebook.com"]::before {
    content: "\f09a";
}
.menu-social-link-container li a[href*="facebook.com"]{
    background: #3a5898;
}

.menu-social-link-container li a[href*="twitter.com"]::before {
    content: "\f099";
}
.menu-social-link-container li a[href*="twitter.com"]{
    background: #00abed;
}

.menu-social-link-container li a[href*="linkedin.com"]::before {
    content: "\f0e1";
}
.menu-social-link-container li a[href*="linkedin.com"]{
    background: #007fae;
}

.menu-social-link-container li a[href*="plus.google.com"]::before {
    content: "\f0d5";
}
.menu-social-link-container li a[href*="plus.google.com"]{
    background: #d04735;
}

.menu-social-link-container li a::before {
    color: #fff;
    font-family: "FontAwesome";
    font-size: 26px;
}
.menu-social-link-container li a:hover{
    background: #ca982b;
}



