/*     header section styling from here   */

.site-branding {
    float: left;
    width: 360px;
}
.hgroup-wrap {
    left: 0;
    /*padding: 25px 0;*/
    position: absolute;
    right: 0;
    z-index: 9;
}
.hgroup-wrap .container{
    align-items: center;
    align-items: -webkit-box;
    display: -webkit-box;      /* OLD - iOS 6-, Safari 3.1-6 */
    display: -moz-box;         /* OLD - Firefox 19- (buggy but mostly works) */
    display: -ms-flexbox;      /* TWEENER - IE 10 */
    display: -webkit-flex;     /* NEW - Chrome */
    display: flex; 
    flex-wrap: wrap;
    flex-wrap: -webkit-box;
    position: relative;
}
.site-title {
    position: relative;
    display: inline-block;
    margin: 23px 0 0;
    font-size: 2.4em;
    font-size: 1.75rem;
    line-height: 1.75rem;
    font-weight: 300;
    text-align: left;
    text-shadow: 0 1px rgba(255,255,255,0.7);
    font-family: "UnitedSans", "Helvetica Neue", Helvetica, Arial, Verdana, sans-serif;
}
.site-title a::before {
    top: 0;
    bottom: 0;
    right: -2em;
    left: -100vw;
    z-index: -1;
    background: #2b364f;
}
.site-title a::after {
    top: 19px;
    right: -4.2em;
    width: 72px;
    height: 23px;
    z-index: -1;
    -webkit-animation-duration: 1s;
    animation-duration: 1s;
    background: #b2b4b7;
}
.site-title::before {
    bottom: -23px;
    height: 23px;
    right: 67%;
    left: -100vw;
    -webkit-animation-duration: 0.75s;
    animation-duration: 0.75s;
    background: #b2b4b7;
}
.site-title::after {
    top: -23px;
    height: 23px;
    right: calc(37% - 1em);
    -webkit-animation-duration: 0.6s;
    animation-duration: 0.6s;
    left: -100vw;
    background: #ca982b;
}
.site-title::before, 
.site-title::after, 
.site-title a::before, 
.site-title a::after {
    content: '';
    display: block;
    position: absolute;
    -webkit-transform: skewX(-30deg) translateZ(0);
    transform: skewX(-30deg) translateZ(0);
    -webkit-animation: 0.5s ease-out 0s 1 slideInHeader;
    animation: 0.5s ease-out 0s 1 slideInHeader;
}
.site-title::before, 
.site-title::after, 
.site-title a::before, 
.site-title a::after {
    content: '';
    display: block;
    position: absolute;
    -webkit-transform: skewX(-30deg) translateZ(0);
    transform: skewX(-30deg) translateZ(0);
    -webkit-animation: 0.5s ease-out 0s 1 slideInHeader;
    animation: 0.5s ease-out 0s 1 slideInHeader;
}
.site-title a {
    position: relative;
    display: inline-block;
    z-index: 10;
    padding: 30px 0 35px;
    border: none;
    color: #0c2340;
    text-decoration: none;
}
a,
a:hover{
    transition: all 0.4s ease 0s;
    -webkit-transition: all 0.4s ease 0s;
    -moz-transition: all 0.4s ease 0s;
    -ms-transition: all 0.4s ease 0s;
    -o-transition: all 0.4s ease 0s;
}


/*    banner section styling from here    */

.banner-section {
    background-clip: border-box;
    background-color: rgba(0, 0, 0, 0);
    /*background-image: url("images/banner-img.png");*/
    background-origin: padding-box;
    background-repeat: no-repeat;
    background-size: cover;
    padding: 265px 0 90px;
    position: relative;
}
.banner-content-section {
    text-align: center;
    width: 880px;
    max-width: 100%;
    position: relative;
    transform: translate(-50%, 0%);
    -moz-transform: translate(-50%, 0%);
    -webkit-transform: translate(-50%, 0%);
    -o-transform: translate(-50%, 0%);
    -ms-transform: translate(-50%, 0%);
}
.banner-content-section h2 {
    font-size: 58px;
    color: #fff;
    margin-bottom: 20px;
    font-family: 'UnitedSansReg-Bold';
    text-transform: uppercase;
}
.banner-content-section h3 {
    font-size: 40px;
    color: #fff;
    text-transform: uppercase;
    margin-bottom: 40px;
    font-family: 'UnitedSansReg-Bold';
}
.highlight-banner-content {
    background: rgb(202, 152, 43) none repeat scroll 0 0;
    color: rgb(255, 255, 255);
    display: inline-block;
    padding: 8px 12px 0px 90px;
    position: relative;
    margin-bottom: 5px;
    min-height: 38px;
    margin-bottom: 25px;
}
.highlight-banner-content::before {
    border-right: 20px solid rgb(202, 152, 43);
    border-top: 38px solid rgba(255, 255, 255, 0);
    content: "";
    display: block;
    height: 38px;
    left: -20px;
    position: absolute;
    top: 0;
}
.highlight-banner-content::after {
    border-left: 20px solid rgb(202, 152, 43);
    border-bottom: 38px solid rgba(255, 255, 255, 0);
    content: "";
    display: block;
    height: 38px;
    right: -20px;
    position: absolute;
    top: 0;
}
.highlight-banner-content span {
    background: #192838;
    position: absolute;
    height: 38px;
    transform: skewX(-30deg) translateZ(0);
    top: 0;
}
.highlight1 {
    width: 16px;
    left: -5px;
}
.highlight2 {
    width: 8px;
    left: 25px;
}
.highlight3 {
    width: 2px;
    left: 55px;
}
.highlight-banner-content h3 {
    font-size: 18px;
    font-weight: 500;
    text-transform: uppercase;
    color: #001841;
    font-family: 'Source Sans Pro', sans-serif;
    margin: 0;
}
.box-button {
    font-size: 25px;
    background: #C7972C;
    color: #fff;
    text-transform: uppercase;
    padding: 16px 50px 13px;
    font-family: 'UnitedSansReg-Heavy';
}
.box-button:hover{
    background: #0C2340;
    color: #fff;
}

