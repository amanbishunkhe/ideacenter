/* Make sure embeds and iframes fit their containers */

embed,
iframe,
object {
    max-width: 100%;
    width: 100%;
}


/*--------------------------------------------------------------
12.1 Captions
--------------------------------------------------------------*/

.wp-caption {
    margin-bottom: 1.5em;
    max-width: 100%;
}

.wp-caption img[class*="wp-image-"] {
    display: block;
    margin: 0 auto;
}

.wp-caption-text {
    text-align: center;
}

.wp-caption .wp-caption-text {
    margin: 0.8075em 0;
}


