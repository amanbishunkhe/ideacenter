    content: "";
    display: table;
}

.clear:after,
.entry-content:after,
.comment-content:after,
.site-header:after,
.site-content:after,
.site-footer:after {
    clear: both;
}


/*--------------------------------------------------------------
9.0 Widgets
--------------------------------------------------------------*/

.widget {
    margin: 0 0 30px;
}
.site-footer .widget {
    margin: 0 0 20px;
}
.widget.mc4wp-form-wrapper{
    margin-bottom: 0
}


/* Make sure select elements fit in widgets */

.widget select {
    max-width: 100%;
}

.widget.wen_corporate_social_widget {
    clear: both;
}


/* Search widget */


/*--------------------------------------------------------------
10.0 Content
--------------------------------------------------------------*/

.entry-content img {
    margin-bottom: 15px;
}

.entry-content {
    margin-bottom: 15px;
}

.entry-meta span {
    border-right: 1px solid;
