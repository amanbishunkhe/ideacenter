/*--------------------------------------------------------------
    Menu Start
--------------------------------------------------------------*/

.main-navigation {}

.main-navigation:before,
.main-navigation:after,
.mc4wp-form-fields:before,
.mc4wp-form-fields:after,
.site-generator-menu:before,
.site-generator-menu:after,
.team-item-wrapper:before,
.team-item-wrapper:after,
.story-item-wrapper:before,
.story-item-wrapper:after,
.brand-content-wrapper:after, 
.brand-content-wrapper:before,
.quick-contact-item:after,
.quick-contact-item:before,
.contact-checkbox-wrap:before,
.contact-checkbox-wrap:after,
.member-item-wrapper:before, 
.member-item-wrapper:after{

    clear: both;
    content: "";
    display: block;
}

.main-navigation ul {
    /*    float: left;*/
    /*text-align: right;*/
}

.main-navigation ul ul {
    background: rgb(255, 255, 255) none repeat scroll 0 0;
    border-top: 31px solid rgba(255, 255, 255, 0);
    display: none;
    left: 0;
    margin: 0;
    min-width: 250px;
    padding: 0;
    position: absolute;
    top: 23px;
    z-index: 9999;
}
.main-navigation ul ul ul{
    border: none;
}

.main-navigation ul ul>li:first-child {
    border-top: 4px solid rgb(216, 221, 56);
}

.main-navigation ul ul ul {
    left: 100%;
    top: -1px;
}

.main-navigation li {
    position: relative;
    display: inline-block;
    margin-bottom: 0;
}

.main-navigation li li {
    float: none;
    display: block;
}

.main-navigation li li li {}

.main-navigation li li li li {}

.main-navigation li:hover ul {
    display: block;
}

.main-navigation li:hover li ul {
    display: none;
}

.main-navigation li li:hover ul {
    display: block;
}

.main-navigation li li:hover li ul {
    display: none;
}

.main-navigation li li li:hover ul {
    display: block;
}

.main-navigation li li li:hover li ul {
    display: none;
}

.main-navigation li li li li:hover>ul {
    display: block;
}
.main-navigation > ul > li {
    margin: 42px 17px;
}

.main-navigation>ul>li:last-child {
    margin-right: 0;
}

.main-navigation ul li a {
    display: block;
    font-size: 16px;
    position: relative;
    text-decoration: none;
    text-transform: uppercase;
    transition: all 0.5s ease-in-out 0s;
    z-index: 99;
}

.main-navigation li li li a {}

.main-navigation li li li li a {}

.main-navigation .current_page_item a,
.main-navigation .current-menu-item a {
    color: rgb(255, 255, 1);
}

.main-navigation li li a:hover,
.main-navigation li li.current-menu-item>a {}

.main-navigation li li li a:hover {}

.main-navigation li li li li a:hover {}

.main-navigation li.current-menu-item>a {}

.main-navigation li li.current-menu-item>a {
    background: #03548b;
    color: #fff;
}

.main-navigation li li li.current-menu-item>a {}

.main-navigation li.current-menu-item li a:hover {
    background: rgb(3, 84, 139);
    color: rgb(255, 255, 255);
}

.main-navigation li li.current-menu-item li a:hover {}

.main-navigation li li li.current-menu-item li a:hover {}

.main-navigation ul ul a {
    border-bottom: 1px solid rgb(91, 91, 91);
    height: auto;
    padding: 8px 15px;
    text-align: left;
    font-size: 16px;
    width: 100%;
}

.main-navigation ul ul a,
.main-navigation li.current-menu-item li a {
    color: #423d3d;
}

.main-navigation ul li ul li a:hover {
    background-color: rgb(3, 84, 139);
    color: hsl(0, 0%, 100%);
    box-shadow: none;
}


/*.main-navigation ul li.menu-item-has-children > a {
    padding-left: 30px;
}*/

.main-navigation li li.current_page_item a:after,
.main-navigation ul li li a:hover:after {
    display: none;
}

.main-navigation li a:hover,
.main-navigation li.current-menu-item a,
.main-navigation li.current_page_item a {
    color: #d8dd38;
}

.main-navigation .menu>li:hover:after,
.main-navigation li.current-menu-item:after,
.main-navigation .menu ul>li:hover:after,
.main-navigation li.current_page_item:after {
    width: 100%;
}

.main-navigation .menu>li:after,
.main-navigation ul li:after,
.main-navigation li.current-menu-item:after,
.main-navigation .menu ul>li:after,
.main-navigation li.current_page_item:after {
    /*    background-color:hsla(0, 0%, 0%, 0.2);*/
    bottom: 0px;
    content: "";
    display: block;
    height: 100%;
    left: 0;
    margin: 0 auto;
    position: absolute;
    transition: width 0.25s ease 0s;
    width: 0;
    z-index: 88;
}

.main-navigation ul li li:after {
    display: none;
}

.main-navigation li.current-menu-item:after,
.main-navigation ul li.current_page_item:after {
    width: 100%;
}




/*--------------------------------------------------------------
        Menu End
