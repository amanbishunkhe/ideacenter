}
.testimonial-section::before {
    background: #2b364f;
    content: "";
    display: block;
    height: 125px;
    margin: auto;
    position: absolute;
    top: 119px;
    width: 30%;
    left: -7vw;
    transform: skewX(-30deg) translateZ(0);
    -webkit-transform: skewX(-30deg) translateZ(0);
    -moz-transform: skewX(-30deg) translateZ(0);
